{
    "compilerOptions": {
      "lib": ["ES2023"],
      "module": "commonjs",
      "target": "ES2022"
      "moduleResolution": "node",
      "incremental": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "declaration": true,
      "sourceMap": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "strict": true,
      "noImplicitAny": true,
      "removeComments": true,
    },
    "exclude": ["node_modules", "dist"]
  }
